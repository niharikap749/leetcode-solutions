class Solution {
    public int maxProfit(int[] prices) {
        int maxProfit=0;
        int minValue=prices[0];
        for(int i=0;i<prices.length;i++){
            if(prices[i]<minValue){
                minValue=prices[i];
            }if(maxProfit<prices[i]-minValue){
                maxProfit=prices[i]-minValue;
            }
        }
        return maxProfit;
    }
}